# Ambiente de produção
NODE_ENV=production

# Porta (será definida automaticamente pelo Render/Railway)
PORT=4000

# JWT Secret (ALTERAR EM PRODUÇÃO!)
JWT_SECRET=super-secret-jwt-key-change-in-production-12345

# Database (SQLite para desenvolvimento, PostgreSQL para produção)
DATABASE_URL=file:./prod.db

# CORS (permite todas as origens em desenvolvimento)
CORS_ORIGIN=*